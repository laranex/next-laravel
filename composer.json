{
    "name": "laranex/next-laravel",
    "description": "A package which allows you to build a next Laravel Application",
    "keywords": [
        "laranex",
        "laravel",
        "next-laravel",
        "laravel-modular"
    ],
    "homepage": "https://next-laravel.netlify.app",
    "license": "MIT",
    "authors": [
        {
            "name": "Nay Thu Khant",
            "email": "naythukhant644@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.1",
        "spatie/laravel-package-tools": "^1.16.4",
        "illuminate/contracts": "^11.0 || ^10.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.16",
        "nunomaduro/collision": "^7.10 || ^8.1",
        "orchestra/testbench": "^8.23",
        "pestphp/pest": "^2.34.7",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.4",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-phpunit": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Laranex\\NextLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laranex\\NextLaravel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-next-laravel --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laranex\\NextLaravel\\NextLaravelServiceProvider"
            ],
            "aliases": {
                "NextLaravel": "Laranex\\NextLaravel\\Facades\\NextLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
